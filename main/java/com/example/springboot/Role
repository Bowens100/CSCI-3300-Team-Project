package net.codejava;
 
import javax.persistence.*;
 
@Entity
@Table(name = "role")
public class Role {
    @Id
    @Column(name = "role_id")
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Integer id;
     
    private String name;
    public Integer getId() 
    {
        return id;
    }
     private void setId(int id)
    {
        this.id = id;
    }
        public Integer getName() 
    {
        return name;
    }
     private void setName(String id)
    {
        this.name = name;
    }
}